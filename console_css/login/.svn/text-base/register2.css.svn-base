body {position: relative;padding-top:60px;padding-bottom:1px;background: #333 url(/public/images/intro-userBg.jpg) no-repeat center center;background-size: cover;overflow: hidden;}
.account-box-wrapper #country-selector{clip:rect(0,0,0,0)!important}
.account-box{position:relative;left:50%;background:#FFF;box-shadow:0 2px 5px 0 rgba(0,0,0,.2);border-radius:7px;padding-bottom:30px;width:582px;margin-left:-291px;margin-top: 88px;margin-bottom: 88px;}
.account-form-title{padding:30px 35px;border-bottom:solid 1px rgba(151,159,168,.2);margin-bottom:35px}
.account-form-title label{font-size:21px;color:#1499f8}
.account-input-area{margin:0 35px 14px}
.account-input-area input:not(.hasflag){width:100%;height:56px;background:#fff;border:1px solid #eaecee;border-radius:5px;padding:0 20px;font-size:14px;color:#2D3238}
.account-input-area input:not(.hasflag).short{width:360px;float:left}
.account-input-area.hasflag{height:56px;overflow-y:hidden}
.account-input-area.hasflag.has-error{height:84px}
.account-input-area .phone-wrapper{width:300px;height:56px;float:left;border-radius:5px;padding-left:1em;border:1px solid #eaecee;background:#fff;box-sizing:border-box}
.account-input-area .phone-wrapper .countries-selector{line-height:54px}
.account-input-area .phone-wrapper .countries-selector .fa.fa-caret-down{padding-left:.5rem;padding-right:1rem}
.account-input-area .phone-wrapper .countries-selector .flag.X2{vertical-align:middle}
.account-input-area .phone-wrapper #phone{border:0;padding:0;width:210px;height:54px;background:#fff;border-radius:5px;font-size:14px;color:#2D3238;box-sizing:border-box}
.account-input-area .select2.select2-container.select2-container--default{height:0;overflow:hidden}
.account-input-area .select2-container .select2-selection.select2-selection--single{display:none!important}
.account-input-area ::-webkit-input-placeholder{color:#8796a8}
.account-input-area :-moz-placeholder{color:#8796a8}
.account-input-area ::-moz-placeholder{color:#8796a8}
.account-input-area :-ms-input-placeholder{color:#8796a8}
.account-input-area input[type=number]::-webkit-inner-spin-button,.account-input-area input[type=number]::-webkit-outer-spin-button{-webkit-appearance:none;margin:0}
.account-input-area input[type=number]{-moz-appearance:textfield}
.account-input-area .account-send-code{width:104px;height:56px;border:none;border-radius:5px;background:#1499f8;box-shadow:0 2px 2px 0 rgba(45,51,56,.2);font-size:14px;color:#FFF;margin-left:8px;cursor:pointer}
.account-input-area .captcha{width:98px;margin-left:8px;position:relative;top:14px;cursor:pointer}
.account-input-area .format-error{box-shadow:0 0 5px rgba(230,30,11,.5);border:1px solid rgba(230,30,11,.5)}
.account-input-area .format-error-label{font-size:1rem;color:#E61E0B;margin:.5rem auto 1rem;clear:left}
.account-button{margin:28px 35px;background:#1499f8;border-radius:5px;box-shadow:0 2px 2px 0 rgba(45,51,56,.2);width:512px;height:49px;font-size:18px;color:#FFF;border:none;cursor:pointer}
.account-bottom-tip{text-align:center;font-size:14px;color:#8796a8;line-height:20px}
.account-bottom-tip a{color:#1499f8}
.account-button.min{margin: 0px;width: 140px;font-size: 14px;height: 56px;line-height: 56px;float: right;}
.account-input-area.tel input.l {width: 204px;float: left;}
.account-input-area.tel input.r {width: 140px;float: left;margin-left: 15px;}
.account-input-area.l-r input{width: 48.5%;}
.account-input-area.l-r .r{float: right;}
#banner .reset-password,.account-bottom-tip.top-tip{margin-bottom:7px}
#banner .check-box{margin-top:30px;margin-left:35px;position:relative}
#banner .check-box input[type=checkbox]{display:none}
#banner .check-box label{border:1px solid #eaecee;cursor:pointer;position:absolute;width:20px;height:20px;top:0;border-radius:3px;background:#FFF}
#banner .check-box label:after{opacity:0;content:'';height:7px;background:0 0;top:6px;left:5px;border:3px solid #333;border-top:none;border-right:none;-webkit-transform:rotate(-45deg);transform:rotate(-45deg)}
#banner .check-box span{position:relative;left:30px;font-size:14px;color:#8796a8}
#banner .check-box a{float:right;margin-right:35px;color:#1499f8}
#banner .check-box input[type=checkbox]:checked+label:after{position:absolute;width:10px;opacity:1}
#account_twofa .mr-label{margin-right:200px}
.banner-back{position: absolute;}
#footer{box-shadow: 0 0 9px rgba(0,0,0,.3);}
.navigation .list>li.left.a{height: 70px; border-bottom: 3px solid #1499f8;}.navigation .logo {background-size: contain;}