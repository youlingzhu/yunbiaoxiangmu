.invateQrcode-border h3{
    font-size: 14px; margin: 0 auto 20px; color: #fff; text-align: center;
    font-weight: bold; letter-spacing: 1px;
}
.invateQrcode-border dt{
    color: #fff; margin-bottom: 20px; font-size: 12px;
    font-weight: normal; letter-spacing: 1px;
    white-space: nowrap;
}
#applyCode{
    font-size: 14px; height: auto; width: 8em; margin: 0 auto; display: block;
}
.invateQrcode-border dl{
    margin-bottom: 0;
}
#apply-dialog{
    display: none;
    width: 400px;
    height: 330px;
    overflow: auto;
    margin: auto;
    position: absolute; z-index: 101;
    top: 0; left: 0; bottom: 0; right: 0;
    background-color: #f2f2f2; padding: 40px 0 0; 
    border-radius: 6px;
    box-shadow: 0px 0px 40px #333;
}
#apply-dia-mark{
    display: none;
    position: absolute; z-index: 100; width: 100%; height: 100%;
    background-color: #000; opacity: .4; filter:alpha(opacity=40) left: 0; top: 0;
}
button.apply{
    padding-left:40px; padding-right: 40px; margin-right: 10px; 
}
button.cancel{
    border-color: #7f838f; color: #626b8a; padding-left: 20px; padding-right: 20px;
}

form .form-left{
    width: 60%;
}
form .form-right{
    width: 35%;
}
.err-popover-out{
    z-index: 3;
}
.submit-count-box{
    position: absolute;
    /* height: 30px; */
    left: 360px;
    top: 8px;
    width: 100%;
}
.submit-count-box span{
    height: 30px;
    line-height: 30px;
}
.submit-count-box .submit-count{
    color: #9C3;
}
.toTelReg.hide{
    display: none;
}
.toMailReg.hide{
    display: none;
}
.code-phone.hide{
    display: none;
}
.submit-count-box{
    display: none;
}
#go_email_modal{
    display: none;
    padding: 0;
}
#go_email_modal .jqui-dialog{

}
#go_email_modal .jqui-content{
    border-radius: 0;
}
#go_email_modal .jqui-header .close{
    position: relative;
    top: 18px;
    right: 23px;
}
#go_email_modal .jqui-body{
    height: 127px;
    padding: 0 28px;
}
#go_email_modal .jqui-body p{
    height: 60px;
    margin-top: 43px;
    font-size: 12px;
    color: #494949;
}
#go_email_modal .jqui-body p .link{
    color: #4D7CB6 !important;
    outline: none;
}
#go_email_modal .tips-label{
    font-size: 14px;
    color: #494949;
    font-weight: normal;
    cursor: pointer;
}
#go_email_modal .btn-blue{
    padding: 0;
    width: 76px;
    height: 30px;
    background-color: #4F83C3;
    color: #FFFFFF;
    outline: none;
    float: right;
}
#go_email_modal .btn-blue:hover {
    background-color: #416B9E; 
    color: #FFFFFF;
    outline: none;
}
.go_email_modal .ui-widget-header{
    display: none;
}
.code-phone-btn[disabled]{
    background-color: #b6b6b6 !important;
    opacity: 1;
    filter: alpha(opacity=100);
}
#code_phone_count{
    color: #0071bc;
}
.emailnotes{
    display: none;
}
.emailnotes span{
    display: block;
    border-radius: 6px;
    height: 46px;
    font-size: 14px;
    text-align: center;
    background-color: #FFFFFF;
    padding: 0 12px;
    line-height: 46px;
    border: 1px solid;
    color: #FF082C;
}
#codePhoneNum[disabled]{
    background-color: #BDBCBC;
}