.spinner {
  width: 100px;
}
.spinner input {
  text-align: right;
  padding-right: 5px;
  height: 34px;
  width: 220px;
}
.input-group-btn-vertical {
  position: relative;
  white-space: nowrap;
  width: 1%;
  vertical-align: middle;
  display: table-cell;
}
.input-group-btn-vertical > .btn {
  display: block;
  float: none;
  width: 100%;
  max-width: 100%;
  padding: 8px;
  margin-left: -1px;
  position: relative;
  border-radius: 0;
}
.input-group-btn-vertical > .btn:first-child {
  border-top-right-radius: 4px;
}
.input-group-btn-vertical > .btn:last-child {
  margin-top: -2px;
  border-bottom-right-radius: 4px;
}
.input-group-btn-vertical i{
  position: absolute;
  top: 0;
  left: 4px;
}
.spinner-up {
	display: inline-block;
    width: 0;
    height: 0;
    margin-left: 0px;
	margin-top: 6px;
    vertical-align: middle;
    border-bottom: 4px dashed;
    border-bottom: 4px solid\9;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
}
.spinner-down {
	display: inline-block;
    width: 0;
    height: 0;
    margin-left: 0px;
	margin-top: 6px;
    vertical-align: middle;
    border-top: 4px dashed;
    border-top: 4px solid\9;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
}

.icon_close {
    cursor: pointer;
    right: 5px;
    top: 5px;
    font-weight: bold;
    font-size: 20px;
    color: #2d2d2d;
    opacity: 0.3;
    filter: alpha(opacity=30);
    width: 25px;
    text-align: center;
    line-height: 25px;
    float: right;
    font-family: iconfont;
    font-style: normal;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
}

