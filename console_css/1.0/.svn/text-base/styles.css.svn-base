}

.tab-content {
	padding: 15px;
}


/*Navbar*/

.navbar {
    padding: 0;
    margin: 0;
	border: 0;
}

.navbar.navbar-inverse {
    background-color: #373d41;
}

.navbar input {
	border: 0;
