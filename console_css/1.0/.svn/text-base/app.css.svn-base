@charset "utf-8";
/* CSS Document */
html,body{padding:0;margin:0;text-align: center;width: 100%;font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;background:#1fb701; position:relative;}
.logo-app{ width:604px; height:327px; margin:0px auto 183px; background:url(../../images/1.0/logo-app.png) no-repeat center 125px; background-size:609px 327px; padding-top:125px;}
.appbutton{ width:100%; height:123px; margin-bottom:228px;}
.appbut{ display:block; width:709px; height:157px; background-color:#fff; position:absolute; left:50%; margin-left:-346px; border-radius:30px; text-decoration: none; text-align:center;}
.appbut_p{ display:block; width:709px; height:157px; background-color:#fff; position:absolute; left:50%; margin-left:-346px; border-radius:30px; text-decoration: none; text-align:center;}
.appbut p{ color:#1fb701; font-size:60px; width:100%; height:90px; line-height:90px; background:url(../../images/1.0/app-and.png) no-repeat 90px center; text-align:center; margin:38px 0px;}
.appbut_p p{ color:#1fb701; font-size:60px; width:100%; height:90px; line-height:90px; background:url(../../images/1.0/IOS_1.png) no-repeat 90px center; text-align:center; margin:38px 0px;}
.image{ width:705px; height:569px; background:url(../../images/1.0/app-img.png) no-repeat; margin-left:auto; margin-right:auto;}
.end{width:100%; height:50px; margin-top:57px; color:#126701; font-size:30px; margin-bottom:20px;}