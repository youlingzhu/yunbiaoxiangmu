*,
html,
body {
    margin: 0px;
    padding: 0px;
    clear: both;
    font-size: 14px;
    color: #999;
    font-family: PingFang SC, microsoft yahei, Helvetica Neue, Tahoma, Arial, "宋体", Helvetica, sans-serif;
}

a {
    text-decoration: none;
}


a:hover {
    text-decoration: underline;
}

#mobileRegister a{
    color: #26b768;
    font-size: 4vw;
}

input {
    outline: none;
    border: none;
}


/**头部样式**/
header {
    width: 100%;
    height: 100px;
}

.headerBox {
    width: 1188px;
    height: 100px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    margin: auto;
}


/**主体内容**/
.content {
    position: absolute;
    height: 590px;
    width: 100%;
    min-width: 100%;
    max-width: 100%;
    top: 100px;
    background: url("../../images/1.0/login_banner0.png");
    display: flex;
    flex-direction: row;
    align-items: center;
    z-index: -1;
}

.contentBox {
    width: 1188px;
    height: 590px;
    position: absolute;
    left: 0px;
    ;
    right: 0px;
    margin: auto;
}


.backgroundImage,
.loginBox {
    height: 590px;
    display: flex;
    align-items: center;

}

.backgroundImage {
    width: 579px;
    height: 387px;
    flex-direction: row;
    justify-content: flex-end;
    position: absolute;
    left: 0px;
    top: 134px;
    background: url("../../images/1.0/590_banner.png") no-repeat;
}

.login {
    padding: 30px 30px 100px 30px;
    background: #fff;
    margin-left: 200px;
    position: absolute;
    right: 0px;
}

.loginTitle {
    font-size: 24px;
    color: #4ea638;
    padding-bottom: 30px;
}

.loginInputBox {
    width: 350px;
    height: 50px;
    display: flex;
    flex-direction: row;
    border: 1px solid #e2e2e2;
    margin-bottom: 30px;

}

.loginInputBox:hover{
    border: 1px solid #4ea638;
}

.loginInputBox .icon,
.inputBox {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.loginInputBox .icon {
    width: 60px;
    height: 50px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.loginInputBox .icon img {
    width: 20px;
    height: 23px;
}

.loginInputBox .inputBox {
    width: 290px;
    height: 50px;
}

.loginInputBox .inputBox input {
    width: 290px;
    border: none;
    outline: none;
    height: 50px;
    font-size: 14px;
}

button {
    width: 350px;
    height: 40px;
    background: #4ea638;
    border-radius: 4px;
    text-align: center;
    color: #fff;
    font-size: 14px;
    font-family: inherit;
    line-height: 40px;
    border: none;
    cursor: pointer;
    margin-top: 30px;
}

.black {
    border: 1px solid #e5e5e5;
    background: #ffffff;
    color: #333;
    margin-top: 10px;
}


.massageBox {
    width: 350px;
    height: 50px;
    display: flex;
    flex-direction: row;
}

.registerInputBox,
.massageTimeBox {
    height: 50px;
    display: flex;
    flex-direction: row;
    align-items: center;
    border: 1px solid #e2e2e2;

}

.registerInputBox:hover{
    border: 1px solid #4ea638;
}

.registerInputBox .icon {
    height: initial;
    width: 50px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.massageTimeBox {
    width: 110px;
    margin-left: 35px;
    text-align: center;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    cursor: pointer;
}


/**移动端样式**/
.mobileBackground {
    width: 100%;
    height: 200px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    /* background: url("../../images/1.0/logo_bg1.png") no-repeat center 40px; */
    background-size: 83%;
}


.mobileloginBox {
    width: 80%;
    height: 50px;
    margin: auto;
    /*margin-top: 7%;*/
}


.mobileInputAndIconBox {
    width: 100%;
    height: 48px;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1px solid #e5e5e5;
}

.mobileIcon {
    width: 10%;
    height: 48px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.mobileIcon img {
    width: 2vh;
}

.mobileInput {
    width: 90%;
    height: 48px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.mobileInput input {
    width: 100%;
    height: 48px;
    border: none;
    outline: none;
    text-indent: 5%;
}

.mobileButton {
    width: 80%;
    height: 13vw;
    background: #26b768;
    border: none;
    border-radius: 90px;
    color: #fff;
    font-size: 4vw;
    margin: auto;
    margin-top: 20%;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    cursor: pointer;
}


.mobileRegister {
    width: 80%;
    margin: auto;
    margin-top: 5%;
    text-align:center;
    font-size: 4vw;

}

.mobileTip {
    width: 80%;
    color: red;
    display: none;
    position: absolute;
    font-size: 3vw;
    left: 0px;
    right: 0px;
    margin: auto;
    margin-bottom: 5px;
    margin-top:2%;
}

.mobileMessageInputBox{
    width: 80%;
    display: flex;
    flex-direction: row;
    align-items: center;
    margin: auto;
}

.mobileMessageInputBox .mobileInputAndIconBox{
    /*width: 50%;*/
    margin: auto;
}

.mobileMessageInputBox  .messageTimer{
    width:40%;
    height: 50px;
    background: #f3f3f2;
    font-size: 4vw;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.qrcodeBackground{
   width:350px;
   height:250px;
   background:url(https://www.iyunbiao.com/wpa/qrcode) no-repeat 0px center;
   background-position:center;
   background-size:250px 250px;
}
.button_list a{    
    padding: 10px 100px 10px 100px;
    background: #26b768;
    border: 2px solid #38b66d;
    color: #fff;
    border-radius: 30px;
    font-size: 16px;
    margin-top: 34px;
    position: relative;
    left: 60px;
  }