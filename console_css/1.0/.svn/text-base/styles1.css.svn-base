@media screen and (max-width: 600px) {
table {border: 0;}
table thead {display: none;}
table tr {margin-bottom: 10px;display: block;border-bottom: 2px solid #ddd;}
table td {display: block;text-align: right;font-size: 13px;border-bottom: 1px dotted #ccc;}
table td:last-child {border-bottom: 0;}
table td:before {content: attr(data-label);float: left;text-transform: uppercase;font-weight:bold;}}

/*分页*/
.table_pagelist{ float:right;}
.table_left{ float:left;}
.foot_r{ padding:0 0 20px 0 !important;}
.pagination_left{ margin:0px 0px 0px 0px;}
.table_pagelist .pagination li a {margin-right: 5px;border-radius:2px;background: #fff;color: #333;border-color: #ccc;padding: 2px 6px; font-size:14px;}
.arrow {display: inline-block;overflow: hidden;border-left: 5px dashed transparent;border-bottom: 5px dashed transparent;border-right: 5px dashed transparent;border-top: 5px solid #fff;vertical-align: middle;}
.table_pagelist .pagination .arrow2{border-left: 5px dashed transparent;border-bottom: 5px dashed transparent;border-top: 5px dashed transparent;border-right: 5px solid #ccc;
}
.table_pagelist .pagination .arrow3 {border-top: 5px dashed transparent;border-bottom: 5px dashed transparent;border-right: 5px dashed transparent;border-left: 5px solid #ccc;
}
.table_pagelist .pagination .arrow2 {margin: 3px 8px 5px -4px;}
.table_pagelist .pagination .arrow3 {margin: 3px 0 5px 8px;}
.table_pagelist .pagination .active a, .table_pagelist .pagination li a:hover{ background:#22bc64; border:1px solid #22bc64; color:#fff;}
.table_pagelist .pagination li a:hover .arrow2{border-left: 5px dashed transparent;border-bottom: 5px dashed transparent;border-top: 5px dashed transparent;border-right: 5px solid #fff;
} 
.table_pagelist .pagination li a:hover .arrow3 {border-top: 5px dashed transparent;border-bottom: 5px dashed transparent;border-right: 5px dashed transparent;border-left: 5px solid #fff;
}

