<script type="text/javascript">
    $(function(){
        $(".menu_list .nLi h3").click(function(){
            if($(this).parent(".nLi").hasClass("on")){
				
                $(this).next(".sub").slideUp(300,function(){
                    $(this).parent(".nLi").removeClass("on");
					//$(".nLi_choose").hide();
//				    $(".c_c").show();
                });
            }else{
                $(this).next(".sub").slideDown(300,function(){
                    $(this).parent(".nLi").addClass("on");
					//$(".nLi_choose").show();
//				    $(".c_c").hide();
                });
            }
        })
    });
$(function(){
  $('#menu').click(function(){
    if($('#menu_list').is(':hidden')){
      $('#menu_list').show();
    }
    else{
      $('#menu_list').hide();
    }
  })
})

//点击的时候显示或隐藏相应区域
	$(function(){
		$("body").delegate(".list_text", "mouseover", function(){
			var sibTarget = $(this).siblings(".list_text");
			sibTarget.each(function(){
				var t = $(this).attr("data-target");
				var st = $("#" + t);
				st.hide();
			});
			sibTarget.removeClass("active");
			var target = $(this).attr("data-target");
			var tt = $("#" + target);
			tt.show();
			$(this).addClass("active");
		});
	});
	
</script>

